# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'


# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY START - can be moved as a block
def j2objc_core
    pod 'j2objc-core-debug', :configuration => ['Debug'], :path => '../core/build/j2objcOutputs'
    pod 'j2objc-core-release', :configuration => ['Release'], :path => '../core/build/j2objcOutputs'
end
# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY END

target 'Flashcards' do
    platform :ios, '8.3'
    j2objc_core
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Flashcards

end